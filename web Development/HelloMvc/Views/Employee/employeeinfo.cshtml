@model Dictionary<long, string>

<h1>Employee details</h1>

<table class="table">
    <tr>
        <th>Employee Name</th>
        <th>Employee Number</th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>@item.Value</td>
            <td>@item.Key</td>
        </tr>

    }
</table>